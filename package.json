{
  "name": "vuetest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "build-only": "vite build",
    "prepare": "husky install",
    "commit": "lint-staged && git add . && cz-customizable",
    "lint-staged": "lint-staged",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/rxjs": "^9.13.0",
    "animate.css": "^4.1.1",
    "ant-design-vue": "3.3.0-beta.4",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.32",
    "rxjs": "^7.8.0",
    "throttle-debounce": "^5.0.0",
    "vue": "^3.2.47",
    "vue-i18n": "9",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@iconify/vue": "^4.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.13.0",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.8.4",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "src/**/*": [
      "eslint . --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.ts"
    }
  }
}
